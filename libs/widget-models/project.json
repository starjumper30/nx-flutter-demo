{
  "name": "widget-models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/widget-models/src",
  "projectType": "library",
  "targets": {
    "analyze": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "libs/widget-models"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "libs/widget-models"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter format libs/widget-models/*",
        "cwd": "libs/widget-models"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "libs/widget-models"
      }
    },
    "doctor": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter doctor",
        "cwd": "libs/widget-models"
      }
    },
    "build-aar": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-apk": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-appbundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-bundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-ios-framework": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    },
    "build-ipa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "libs/widget-models"
      },
      "outputs": [
        "{workspaceRoot}/libs/widget-models/build"
      ]
    }
  },
  "tags": []
}
